        </div>
      </main>
    </div>
    <%- include('footer') %>
  </div>
  <!-- Media fullscreen viewer -->
  <div class="modal fade" id="fullscreenViewer" tabindex="-1" aria-labelledby="fullscreenViewerLabel" aria-hidden="true">
    <div class="modal-dialog modal-fullscreen">
      <div class="modal-content modal--media-viewer">
        <div class="modal-header">
          <h2 class="modal-title fs-6" id="fullscreenViewerLabel">Перегляд</h2>
          <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Закрити"></button>
        </div>
        <div class="modal-body p-0 d-flex align-items-center justify-content-center">
          <img id="fullscreenViewerImage" src="" alt="" style="max-width:100%;max-height:100vh;object-fit:contain;display:none;" />
          <video id="fullscreenViewerVideo" controls style="max-width:100%;max-height:100vh;display:none;"></video>
          <iframe id="fullscreenViewerFrame" src="" title="Документ" style="width:100%;height:100vh;border:0;display:none;"></iframe>
        </div>
      </div>
    </div>
  </div>
  <button class="back-to-top" type="button" aria-label="Повернутися нагору">
    <i class="fa-solid fa-arrow-up"></i>
  </button>
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL" crossorigin="anonymous"></script>
  <script src="/js/main.js?v=<%= assetVersion %>" defer></script>
</body>
</html>
